[init]
    defaultBranch = main

[pull]
    rebase = false

[user]
    name = Josh
    email = josh@joshthomas.dev

[alias]
    branch-name = "!git rev-parse --abbrev-ref HEAD"
    publish = "!git push -u origin $(git branch-name)"
    co = "checkout"
    cob = "checkout -b"
    del = "branch -D"
    patch = "add -p"

